<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:GameboyPiManager.Views.Resources"
                    xmlns:viewModels="clr-namespace:GameboyPiManager.ViewModels">

    <DataTemplate x:Key="VideogameConsoleListBoxItem" DataType="{x:Type viewModels:VideogameConsoleViewModel}">
        <Border BorderBrush="Black" BorderThickness="1" Background="BurlyWood" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <TextBlock Text="{Binding Name}"/>
        </Border>
    </DataTemplate>
</ResourceDictionary>